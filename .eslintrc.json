{
  "extends": "airbnb",
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "quotes": "off",
    "react/prop-types": 0,
    "arrow-body-style": 0,
    "space-before-function-paren": 0,
    "indent": ["error", 4, "tab"]
  },
  "env": {
    "browser": true
  },
  "parser": "babel-eslint"
}
